{
	"name": "edde-framework/edde-framework",
	"homepage": "https://edde-framework.org",
	"license": "Apache-2.0",
	"type": "library",
	"description": "Fast and full featured modern PHP framework.",
	"keywords": [
		"php",
		"framework"
	],
	"authors": [
		{
			"name": "Marek Hanzal",
			"email": "marek.hanzal@edde-framework.org"
		}
	],
	"config": {
		"optimize-autoloader": true,
		"vendor-dir": "lib"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=7.0",
		"paragonie/constant_time_encoding": "^2.0",
		"defuse/php-encryption": "^2.0",
		"phpunit/phpunit": "6.0.x-dev",
		"theseer/phpdox": "0.9.0",
		"roave/security-advisories": "dev-master",
		"league/uri-parser": "^1.0"
	},
	"require-dev": {
		"tracy/tracy": "^2.4"
	}
}
