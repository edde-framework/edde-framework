<control title="Edde Setup">
	<e:css src="css/kube.css"/>
	<e:css src="css/edde-framework.css"/>
	<e:js src="js/jquery-3.1.0.js"/>
	<e:js src="js/edde-framework.js"/>

	<div id="message" class="alert" m:snippet="message"/>

	<div class="row centered">
		<div class="col col-5">
			<button value="Upgrade" action="on-upgrade()" title="run upgrades registered to this application"/>
			<button value="Rebuild crates" action="on-rebuild-crates()" title="update automagically generated crates"/>
			<button value="Clear cache" action="on-clear-cache()" title="clear current cache"/>
		</div>
	</div>
</control>
